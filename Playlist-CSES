//https://cses.fi/problemset/task/1141/

#include<bits/stdc++.h>
typedef long long ll;
using namespace std;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	
	ll n;cin>>n;
	ll a[n];
	for(ll i=0;i<n;i++) cin>>a[i];
	map<ll,ll> mp;
	mp[a[0]]++;
	ll i=0,j=1,mx=0,c_l=1;
	while(i<n-1 and j<n){
		if(mp[a[j]]==0){
			c_l++;
			mp[a[j]]++;
			j++;
		}
		else{
			mx=max(mx,c_l);
			mp[a[i]]--;
			c_l--;
			i++;
		}
	}
	cout<<max(c_l,mx)<<"\n";
}
